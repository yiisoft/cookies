{
    "name": "yiisoft/cookies",
    "type": "library",
    "description": "Convenient way to use cookies with PSR-7",
    "keywords": [
        "cookies",
        "cookie",
        "psr-7"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/cookies/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/cookies"
    },
    "require": {
        "php": "^7.4|^8.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "1.0",
        "yiisoft/http": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.20.1",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cookies\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cookies\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
